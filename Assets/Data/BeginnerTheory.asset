    \u043E\u0442 \u0442\u0438\u043F\u0430 SDRAM-\u043F\u0430\u043C\u044F\u0442\u0438:
    DDR, DDR2, DDR3, DDR4, DDR5. \u0421\u0432\u044F\u0437\u0430\u043D\u043E \u044D\u0442\u043E
    \u0441 \u0442\u0435\u043C, \u0447\u0442\u043E \u0442\u0438\u043F\u044B \u043F\u0430\u043C\u044F\u0442\u0438
    \u043D\u0435 \u0441\u043E\u0432\u043C\u0435\u0441\u0442\u0438\u043C\u044B \u043C\u0435\u0436\u0434\u0443
    \u0441\u043E\u0431\u043E\u0439."
  - "\u041E\u0431\u044A\u0451\u043C \u043C\u043E\u0434\u0443\u043B\u044F \u043F\u0430\u043C\u044F\u0442\u0438\r\n\r\n\u041E\u0431\u044A\u0451\u043C
    \u043E\u043F\u0435\u0440\u0430\u0442\u0438\u0432\u043D\u043E\u0439 \u043F\u0430\u043C\u044F\u0442\u0438,
    \u043D\u0430 \u0440\u044F\u0434\u0443 \u0441 \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043A\u0430\u043C\u0438
    \u043F\u0440\u043E\u0447\u0438\u0445 \u043A\u043E\u043C\u043F\u043B\u0435\u043A\u0442\u0443\u044E\u0449\u0438\u0445
    \u041F\u041A, \u043D\u0435\u043F\u043E\u0441\u0440\u0435\u0434\u0441\u0442\u0432\u0435\u043D\u043D\u043E
    \u0432\u043B\u0438\u044F\u0435\u0442 \u043D\u0430 \u043F\u0440\u043E\u0438\u0437\u0432\u043E\u0434\u0438\u0442\u0435\u043B\u044C\u043D\u043E\u0441\u0442\u044C
    \u0441\u0438\u0441\u0442\u0435\u043C\u044B \u0432 \u0446\u0435\u043B\u043E\u043C.
    \u041F\u0440\u0438 \u0434\u043E\u0441\u0442\u0430\u0442\u043E\u0447\u043D\u043E\u043C
    \u043E\u0431\u044A\u0451\u043C\u0435 \u041E\u0417\u0423 \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u043E\u043D\u043D\u0430\u044F
    \u0441\u0438\u0441\u0442\u0435\u043C\u0430 \u0440\u0435\u0436\u0435 \u0437\u0430\u0434\u0435\u0439\u0441\u0442\u0432\u0443\u0435\u0442
    \u0444\u0430\u0439\u043B \u043F\u043E\u0434\u043A\u0430\u0447\u043A\u0438, \u0447\u0442\u043E
    \u0438\u0441\u043A\u043B\u044E\u0447\u0430\u0435\u0442 \u043B\u0438\u0448\u043D\u0438\u0435
    \u043E\u043F\u0435\u0440\u0430\u0446\u0438\u0438 \u0447\u0442\u0435\u043D\u0438\u044F/\u0437\u0430\u043F\u0438\u0441\u0438"
  - "\u0422\u0430\u043A\u0442\u043E\u0432\u0430\u044F \u0447\u0430\u0441\u0442\u043E\u0442\u0430
    \u043E\u043F\u0435\u0440\u0430\u0442\u0438\u0432\u043D\u043E\u0439 \u043F\u0430\u043C\u044F\u0442\u0438
    \n\u041F\u0430\u0440\u0430\u043C\u0435\u0442\u0440 \u0437\u0430\u0432\u0438\u0441\u0438\u0442
    \u043E\u0442 \u0442\u0438\u043F\u0430 \u043E\u043F\u0435\u0440\u0430\u0442\u0438\u0432\u043D\u043E\u0439
    \u043F\u0430\u043C\u044F\u0442\u0438: DDR, DDR 2, DDR 3, DDR 4, DDR 5. \u0427\u0435\u043C
    \u0432\u044B\u0448\u0435 \u0442\u0430\u043A\u0442\u043E\u0432\u0430\u044F \u0447\u0430\u0441\u0442\u043E\u0442\u0430,
    \u0442\u0435\u043C \u043B\u0443\u0447\u0448\u0435. \u041E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u043E
    \u0441\u0442\u043E\u0438\u0442 \u0443\u0447\u0438\u0442\u044B\u0432\u0430\u0442\u044C
    \u0445\u0430\u0440\u0430\u043A\u0442\u0435\u0440\u0438\u0441\u0442\u0438\u043A\u0438
    \u043F\u0440\u043E\u0446\u0435\u0441\u0441\u043E\u0440\u0430, \u043A\u043E\u0442\u043E\u0440\u044B\u0439
    \u0434\u043E\u043B\u0436\u0435\u043D \u043F\u043E\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0442\u044C
    \u0441\u043E\u043E\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0443\u044E\u0449\u0443\u044E
    \u0442\u0430\u043A\u0442\u043E\u0432\u0443\u044E \u0447\u0430\u0441\u0442\u043E\u0442\u0443
    \u041E\u0417\u0423. \n\u041E\u0431\u044F\u0437\u0430\u0442\u0435\u043B\u044C\u043D\u043E
    \u0441\u0442\u043E\u0438\u0442 \u0443\u0447\u0438\u0442\u044B\u0432\u0430\u0442\u044C
    \u0440\u0435\u0436\u0438\u043C \u0440\u0430\u0431\u043E\u0442\u044B \u2014 \u043E\u0434\u043D\u043E-
    \u0438\u043B\u0438 \u0434\u0432\u0443\u0445\u043A\u0430\u043D\u0430\u043B\u044C\u043D\u044B\u0439.
    \u0415\u0441\u043B\u0438 \u043F\u0440\u043E\u0446\u0435\u0441\u0441\u043E\u0440
    \u0441\u043F\u043E\u0441\u043E\u0431\u0435\u043D \u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C
    \u0441 \u043C\u0430\u043A\u0441\u0438\u043C\u0430\u043B\u044C\u043D\u043E\u0439
    \u0447\u0430\u0441\u0442\u043E\u0442\u043E\u0439 \u043E\u043F\u0440\u0435\u0434\u0435\u043B\u0451\u043D\u043D\u043E\u0433\u043E
    \u0442\u0438\u043F\u0430 \u043F\u0430\u043C\u044F\u0442\u0438 \u0432 \u043E\u0434\u043D\u043E\u043A\u0430\u043D\u0430\u043B\u044C\u043D\u043E\u043C
    \u0440\u0435\u0436\u0438\u043C\u0435, \u043E\u043D \u043C\u043E\u0436\u0435\u0442
    \u043D\u0435 \u043F\u043E\u0434\u0434\u0435\u0440\u0436\u0438\u0432\u0430\u0442\u044C
    \u0434\u0430\u043D\u043D\u0443\u044E \u0447\u0430\u0441\u0442\u043E\u0442\u0443
    \u0432 \u0434\u0432\u0443\u0445\u043A\u0430\u043D\u0430\u043B\u044C\u043D\u043E\u043C
    \u0440\u0435\u0436\u0438\u043C\u0435. \u041F\u0440\u0438 \u044D\u0442\u043E\u043C,
    \u0441\u0438\u0441\u0442\u0435\u043C\u0430 \u0437\u0430\u043F\u0443\u0441\u0442\u0438\u0442\u0441\u044F
    \u0438 \u0431\u0443\u0434\u0435\u0442 \u0440\u0430\u0431\u043E\u0442\u0430\u0442\u044C,
    \u043D\u043E \u043D\u0430 \u0431\u043E\u043B\u0435\u0435 \u043D\u0438\u0437\u043A\u043E\u0439
    \u0447\u0430\u0441\u0442\u043E\u0442\u0435."
